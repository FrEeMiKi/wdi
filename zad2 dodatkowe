x = int(input("Podaj sumę ciągu Fibonacciego: "))
ciag = [0,1]
suma = int()
while x > suma:
    ciag.append(ciag[-1]+ciag[-2])
    suma = sum(ciag)
if suma == x:
    print(f"Istnieje podciąg ciągu Fibonacciego o sumie {x}")
else:
    print(f"Nie ma podciągu ciągu Fibonacciego o sumie {x}. Najblizsza wartość takiego podciągu wynosi {min(suma,suma-ciag[-1])}")
